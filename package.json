{
  "name": "MDR",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "dev": "react-native start --reset-cache",
    "dev:android": "npm run dev & npm run android",
    "dev:hot": "react-native start --reset-cache",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:release": "cd android && ./gradlew assembleRelease",
    "clean": "cd android && ./gradlew clean && cd .. && npx react-native start --reset-cache",
    "adb:connect": "adb reverse tcp:8081 tcp:8081",
    "adb:devices": "adb devices",
    "logs:android": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "reset": "npx react-native start --reset-cache --verbose",
    "deadcode:check": "ts-prune --error",
    "deadcode:find": "ts-prune",
    "deadcode:lint": "eslint . --max-warnings=0",
    "deadcode:typescript": "tsc --noEmit --strict",
    "deadcode:all": "npm run deadcode:typescript && npm run deadcode:lint && npm run deadcode:check",
    "knip": "knip",
    "knip:unused-files": "knip --include files",
    "knip:unused-exports": "knip --include exports",
    "knip:unused-dependencies": "knip --include dependencies",
    "knip:production": "knip --production",
    "knip:fix": "knip --fix",
    "validate": "node scripts/validate-build.js",
    "build:test": "npm run validate && npm run build:android",
    "build:validate": "npm run validate",
    "prebuild": "npm run validate",
    "ci": "npm run validate && npm run build:android",
    "precommit": "npm run validate"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native/new-app-screen": "0.81.0",
    "expo": "^53.0.22",
    "react": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-sound": "^0.12.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@types/jest": "^29.5.13",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^29.6.3",
    "knip": "^5.63.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
